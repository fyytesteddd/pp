if x:IsA("ModuleScript") then
local y, z =
pcall(
function()
return require(x)
end
)
if y and z then
local A = z.Data or z
if A and type(A) == "table" and A.Id and A.Name then
v = v + 1
n[A.Id] = A.Name
o[A.Id] = A.Tier or 1
if z.SellPrice then
p[A.Id] = z.SellPrice
elseif A.SellPrice then
p[A.Id] = A.SellPrice
else
p[A.Id] = 0
end
end
end
end
end
return v
end
