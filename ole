 Title="Teleport To Game Event",
 Default=on,
 Callback=function(s)
  on=s
  getgenv().HuntTeleportSettings.isTeleporting=s
  if s then task.wait(.5) start() else stop() end
 end
}))

P.CharacterAdded:Connect(function()
 task.wait(2)
 if on then task.wait(1) start() end
end)

