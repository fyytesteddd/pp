else
WindUI:Notify({Title="Failed",Content="Player not found or no character",Duration=3,Icon="x"})
end
else
WindUI:Notify({Title="Failed",Content="Select a player first",Duration=3,Icon="alert-circle"})
end
end
})
task.spawn(function()
task.wait(1)
refreshPlayerList()
end)
