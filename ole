if ak.Data.Icon then
a0 = tonumber(string.match(tostring(ak.Data.Icon), "%d+")) or 0
elseif ak.Data.ImageId then
a0 = tonumber(ak.Data.ImageId) or 0
end
end
local at = _(a0)
